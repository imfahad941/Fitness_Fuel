<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="ExtraText"
    android:orientation="vertical"><![CDATA[

    tools:context=".MainActivity">









    ]]>

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolBar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/primary"
        android:gravity="end"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:textAlignment="textEnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:title="Fitness Fuel"
        app:titleTextColor="#FFF"
        tools:ignore="MissingConstraints"></androidx.appcompat.widget.Toolbar>



<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


 <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:orientation="vertical">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="242dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="163dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="178dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv4"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:text="Before Age 18"
                    android:textColor="#090808"
                    android:textSize="27dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:text="60 Days"
                    android:textSize="24dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:text="Every Day 15 MIN"
                    android:textColor="#EC673D"
                    android:textSize="18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="131dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="131dp"
                    android:layout_height="149dp"
                    app:srcCompat="@drawable/exersice_1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="63dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/arrow1"
                    android:layout_width="67dp"
                    android:layout_height="147dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />
            </LinearLayout>

        </LinearLayout>

        <Button
            android:id="@+id/startyoga1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/btn"
            android:text="Get Started"
            android:textSize="18dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="251dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="163dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="178dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:text="Before Age 18"
                    android:textColor="#090808"
                    android:textSize="27dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv2"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:text="60 Days"
                    android:textSize="24dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv3"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:text="Every Day 15 MIN"
                    android:textColor="#EC673D"
                    android:textSize="18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="127dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/iv1"
                    android:layout_width="127dp"
                    android:layout_height="149dp"
                    app:srcCompat="@drawable/exersice_13" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="63dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/arrow2"
                    android:layout_width="67dp"
                    android:layout_height="147dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />
            </LinearLayout>

        </LinearLayout>

        <Button
            android:id="@+id/startyoga2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/btn"
            android:text="Get Started"
            android:textSize="18dp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="163dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="178dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv5"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:text="Nutrition or Tips"
                    android:textColor="#090808"
                    android:textSize="27dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv6"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:text="Suggestions for Free"
                    android:textSize="20dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="123dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/iv3"
                    android:layout_width="123dp"
                    android:layout_height="149dp"
                    app:srcCompat="@drawable/food" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="63dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/arrow3"
                    android:layout_width="67dp"
                    android:layout_height="147dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_baseline_keyboard_arrow_right_24" />
            </LinearLayout>

        </LinearLayout>


    </LinearLayout>
 </LinearLayout>
</ScrollView>



</LinearLayout>





